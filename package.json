{
  "name": "ngx-badges",
  "version": "0.0.0-development",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run build:showcase",
    "copy:readme": "copyfiles ./README.md ./dist/ngx-badges",
    "build:lib": "npm run copy:readme && ng build ngx-badges",
    "build:showcase": "ng build ng-badges-showcase --base-href='https://jtc10005.github.io/ngx-badges/'",
    "publish:demo": "npx angular-cli-ghpages --dir=./dist/ng-badges-showcase",
    "test:lib-coverage": "ng test ng-alert --code-coverage --watch=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "projects/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "~11.2.6",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.18",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codecov": "^3.8.3",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.4.1",
    "husky": "^8.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "1.5.0",
    "lint-staged": "^12.4.1",
    "ng-packagr": "^11.0.0",
    "prettier": "^2.6.2",
    "protractor": "~7.0.0",
    "semantic-release": "^19.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jtc10005/ng-badges.git"
  }
}
